{
  "name": "markdown-code-snippet-runner",
  "displayName": "Markdown Code Terinal Executor",
  "description": "Run single quote code sippets in markdown",
  "version": "1.0.0-beta",
  "publisher": "jackbilestech",
  "license": "MIT",
  "engines": {
    "vscode": "^1.32.0"
  },
  "repository": {
    "url": "https://github.com/jackbilestech/vscode-readme-code-snippet-runner"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "assets/icon.png",
  "keywords": [
    "shell",
    "terminal",
    "markdown"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run lint && npm run test",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "test": "exit 0",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vscode/test-electron": "^2.0.0",
    "@vscode/vsce": "^2.15.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.4"
  }
}